<form method="post" action="{{ path('app_admin_cat_delete', {'idCat': categorie.idCat}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.idCat) }}">
    <button class="btn btn-success" style="background-color: #778A17; ">Delete</button>
</form>
